{
    "useTabs": true,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ],
    "svelteSortOrder": "options-scripts-styles-markup",
    "svelteStrictMode": true,
    "svelteBracketNewLine": false,
    "svelteAllowShorthand": true,
    "svelteIndentScriptAndStyle": true
  }
  